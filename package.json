{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "start": "npm run prettier & tsc & node build/server.js",
    "watch": "npm run prettier & tsc-watch --noClear -p tsconfig.json --onSuccess \"node ./build/server.js\"",
    "test": "db-migrate up -e test & npm run build & jasmine-ts & db-migrate reset -e test",
    "lint": "eslint . --ext .ts",
    "prettier": "npx prettier --config .prettierrc --write ./src/**/*.ts",
    "build": "tsc --build",
    "db": "docker compose up"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "supertest": "^6.2.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
